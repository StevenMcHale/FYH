{
  "version": 2,
  "builds": [
    {
      "src": "uni_planner/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1",
      "headers": {
        "cache-control": "public, max-age=3600, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "uni_planner/wsgi.py"
    }
  ],
  "env": {
    "DEBUG": "False"
  }
}
